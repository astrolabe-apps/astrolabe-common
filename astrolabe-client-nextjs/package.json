	},
	"devDependencies": {
		"@react-typed-forms/transform": "^0.2.0",
		"@babel/core": "^7.23.7",
		"@babel/cli": "^7.23.4",
		"@babel/preset-env": "^7.23.8",
		"@babel/preset-react": "^7.23.3",
		"@babel/preset-typescript": "^7.23.3",
		"microbundle": "^0.15.1",
		"typescript": "^5.6.2",
		"rimraf": "^3.0.2",
		"@types/react": "^18.2.33",
		"@types/node": "^20.8.4",
		"prettier": "^3.0.3"
	}
}
