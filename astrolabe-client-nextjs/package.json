{
	"name": "@astroapps/client-nextjs",
	"version": "1.1.0",
	"type": "module",
	"main": "lib/index.cjs",
	"module": "lib/index.js",
	"types": "lib/index.d.ts",
	"exports": {
		"types": "./lib/index.d.ts",
		"require": "./lib/index.cjs",
		"default": "./lib/index.js"
	},
	"author": "Astrolabe Enterprises",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/astrolabe-apps/astrolabe-common/issues"
	},
	"homepage": "https://github.com/astrolabe-apps/astrolabe-common#readme",
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"@astroapps/client": "workspace:^1.1.0",
		"querystring": "^0.2.1",
		"next": "14.0.4",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"tslib": "^2.6.2"
	},
	"scripts": {
		"prepack": "npm run build",
		"build": "rimraf ./lib/ && microbundle -f modern,cjs --jsx React.createElement --jsxFragment React.Fragment",
