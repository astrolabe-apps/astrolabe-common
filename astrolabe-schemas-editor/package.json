{
  "name": "@astroapps/schemas-editor",
  "version": "10.5.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Astrolabe Enterprises",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/astrolabe-apps/astrolabe-common/issues"
  },
  "homepage": "https://github.com/astrolabe-apps/astrolabe-common#readme",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepack": "npm run build",
    "build": "rimraf ./lib/ && microbundle -f cjs --no-compress --jsx React.createElement --jsxFragment React.Fragment",
    "watch": "microbundle -w -f cjs --no-compress --jsx React.createElement --jsxFragment React.Fragment",
    "genschemas": "h get http://localhost:5000/CodeGen/Schemas > src/schemaSchemas.ts && prettier -w src/schemaSchemas.ts"
  },
  "dependencies": {
    "@react-typed-forms/core": "^3.5.2",
    "react-resizable-panels": "^0.0.55",
    "@react-typed-forms/schemas": "workspace:^11.5.1",
    "@astroapps/client": "workspace:^0.1.8",
    "@astroapps/ui-tree": "workspace:^1.0.0",
    "@dnd-kit/core": "^6.0.5",
    "@mhsdesign/jit-browser-tailwindcss": "^0.4.0",
    "react": "^18.2.0",
    "immutability-helper": "^3.1.1",
    "clsx": "^2",
    "react-hook-inview": "^4.5.1"
  },
  "devDependencies": {
    "@react-typed-forms/transform": "^0.2.0",
    "@babel/core": "^7.23.7",
    "@babel/cli": "^7.23.4",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@types/react": "^18.2.31",
    "rimraf": "^5.0.5",
    "typescript": "5.2.2",
    "prettier": "^3.0.3",
    "http-request-cli": "^0.2.0",
    "react-dom": "^18.2.0",
    "microbundle": "^0.15.1"
  }
}
