    "@mhsdesign/jit-browser-tailwindcss": "^0.4.2",
    "immutability-helper": "^3.1.1",
    "clsx": "^2",
    "react-arborist": "^3.4.3",
    "use-resize-observer": "~9.1.0",
    "react-hook-inview": "^4.5.1",
