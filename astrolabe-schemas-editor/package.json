    "react": "^18.2.0 || ^19",
    "react-dom": "^18.2.0 || ^19",
    "react-dnd": "^16.0.1",
    "@dnd-kit/core": "^6.0.5",
    "@react-typed-forms/transform": "^0.2.0",
    "@babel/core": "^7.23.7",
